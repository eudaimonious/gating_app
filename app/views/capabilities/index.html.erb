


    <div class="container">


<!-- Log In Links
================================================== -->
<header class="jumbotron subhead" id="overview">
  <div class="row">
    <div class="span6">
           <% if not user_signed_in? %>
                <h1><%= link_to "Register", new_user_registration_path %> </h1>
                <h1><%= link_to "Sign In", new_user_session_path %></h1>
            <% end %>

   </div>
  </div>
</header>

<!-- Capabilities Table
================================================== -->

<% if user_signed_in? %>

      <div class="bs-docs-section">

        <div class="row">
          <div class="col-lg-12">
            <div class="page-header">
              <h1 id="tables"><%=current_user.organization.full_name%> Capabilities</h1>
            </div>

            <div class="bs-example">
              <table class="table table-striped table-bordered table-hover">
                <thead>
                  <tr>
                    <th>Capability Name</th>
                    <th>Capability Key</th>
                    <th>Capability Setting</th>
                  </tr>
                </thead>
                <tbody>
                  <% @capabilities.each do |capability| %>
                  <tr>
                    <td><%= capability.name %></td>
                    <td><%= capability.key %></td>
                    <td>Coming Soon</td>
                  </tr>
                  <% end %>
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
<% end %>


