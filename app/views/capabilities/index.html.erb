


    <div class="container">


<!-- Log In Links
================================================== -->
<header class="jumbotron subhead" id="overview">
  <div class="row">
    <div class="span6">
           <% if not user_signed_in? %>
                <h1><%= link_to "Register", new_user_registration_path %> </h1>
                <h1><%= link_to "Sign In", new_user_session_path %></h1>
            <% end %>

   </div>
  </div>
</header>

<!-- Capabilities List
================================================== -->

<% if user_signed_in? %>
<section id="capabilities-list">
  <div class="page-header">

    <h1><%=current_user.organization.full_name%> Capabilities</h1>
  </div>

  <!-- Headings & Paragraph Copy -->
  <div class="row">

    <div class="span12">
      <div class="well">
        <% @capabilities.each do |capability| %>
        	<% if current_user.organization == capability.organizations.id %>
          		<h1><%= capability.name %></h1>
          	<% end %>
        <% end %>
      </div>
    </div>
  </div>


</section>
<% end %>
